<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FinalOutcome">

    <TextView
        android:id="@+id/txtfinaloutcome"
        android:layout_width="188dp"
        android:layout_height="51dp"
        android:layout_marginTop="284dp"
        android:text="TextView"
        android:fontFamily="@font/alphakind"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnLastFinal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="88dp"
        android:text="Continue"
        android:fontFamily="@font/alphakind"
        app:layout_constraintEnd_toEndOf="@+id/txtfinaloutcome"
        app:layout_constraintHorizontal_bias="0.485"
        app:layout_constraintStart_toStartOf="@+id/txtfinaloutcome"
        app:layout_constraintTop_toBottomOf="@+id/txtfinaloutcome" />
</androidx.constraintlayout.widget.ConstraintLayout>